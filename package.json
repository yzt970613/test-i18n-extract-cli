{
  "name": "taro-vue3-nutui-template",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "templateInfo": {
    "name": "vue3-NutUI4",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "cross-env MY_ENV=prod taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "cross-env MY_ENV=prod taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "cross-env MY_ENV=test taro build --type weapp --watch",
    "dev:weapp:compress": "NODE_ENV=production cross-env MY_ENV=test npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "cross-env MY_ENV=test taro build --type h5 --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "---": "---",
    "postinstall": "simple-git-hooks",
    "lint": "eslint './**/*.{vue,ts,tsx,mts,cts,js,jsx,mjs,cjs}'",
    "lint:fix": "eslint --fix './**/*.{vue,ts,tsx,mts,cts,js,jsx,mjs,cjs}'",
    "prettier": "prettier --check './**/*.{vue,ts,tsx,mts,cts,js,jsx,mjs,cjs,json,md,css,less,scss}' --ignore-unknown --no-error-on-unmatched-pattern",
    "prettier:fix": "prettier --write './**/*.{vue,ts,tsx,mts,cts,js,jsx,mjs,cjs,json,md,css,less,scss}' --ignore-unknown --no-error-on-unmatched-pattern",
    "i18nxt": "i18nxt"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@ktaro/components": "^0.0.32",
    "@ktaro/components-resolver": "^0.0.32",
    "@ktaro/inject-component-loader": "^0.0.32",
    "@ktaro/router-interceptor": "^0.0.32",
    "@ktaro/shared": "^0.0.32",
    "@ktaro/tailwindcss": "^0.0.32",
    "@ktaro/themes": "^0.0.32",
    "@nutui/icons-vue-taro": "^0.0.9",
    "@nutui/nutui-taro": "^4.1.2",
    "@tarojs/components": "3.6.8",
    "@tarojs/helper": "3.6.8",
    "@tarojs/plugin-framework-vue3": "3.6.8",
    "@tarojs/plugin-html": "3.6.8",
    "@tarojs/plugin-http": "3.6.8",
    "@tarojs/plugin-platform-alipay": "3.6.8",
    "@tarojs/plugin-platform-h5": "3.6.8",
    "@tarojs/plugin-platform-jd": "3.6.8",
    "@tarojs/plugin-platform-qq": "3.6.8",
    "@tarojs/plugin-platform-swan": "3.6.8",
    "@tarojs/plugin-platform-tt": "3.6.8",
    "@tarojs/plugin-platform-weapp": "3.6.8",
    "@tarojs/runtime": "3.6.8",
    "@tarojs/shared": "3.6.8",
    "@tarojs/taro": "3.6.8",
    "axios": "^1.4.0",
    "i18next": "^23.4.5",
    "i18next-vue": "^3.0.0",
    "pinia": "2.0.36",
    "vue": "^3.2.40"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@tarojs/cli": "3.6.8",
    "@tarojs/taro-loader": "3.6.8",
    "@tarojs/webpack5-runner": "3.6.8",
    "@types/node": "^18.15.11",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-preset-taro": "3.6.8",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "3.4.2",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.15.1",
    "lint-staged": "^10.2.10",
    "postcss-rem-to-responsive-pixel": "^5.1.3",
    "prettier": "^2.8.8",
    "simple-git-hooks": "^2.8.1",
    "style-loader": "1.3.0",
    "stylelint": "9.3.0",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.1.0",
    "unplugin-vue-components": "^0.23.0",
    "vue-loader": "^17.0.0",
    "weapp-tailwindcss": "^2.5.2",
    "webpack": "^5.78.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,vue}": [
      "prettier --write",
      "eslint --cache --fix"
    ],
    "*.{json,md,css,less,scss}": "prettier --write"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn run lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "engines": {
    "node": ">=16.14.0"
  }
}
